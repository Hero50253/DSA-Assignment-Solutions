#include<stdio.h>

int main(){
    int arr[50];
    int size,i,j,k;
    
    printf("enter size: ");
    scanf("%d",&size);
    
    printf("enter elements: ");
    for(i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    
    // remove duplicates
    for(i=0;i<size;i++){
        for(j=i+1;j<size;j++){
            if(arr[i]==arr[j]){
                // shift left
                for(k=j;k<size-1;k++){
                    arr[k]=arr[k+1];
                }
                size--;
                j--;
            }
        }
    }
    
    printf("after removing duplicates: ");
    for(i=0;i<size;i++){
        printf("%d ",arr[i]);
    }
    
    return 0;
}
